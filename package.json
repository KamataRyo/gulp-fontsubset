{
  "name": "gulp-fontsubset",
  "version": "0.0.0",
  "description": "gulp-fontsubset make subset from ttf.",
  "main": "lib/gulp-fontsubset.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel ./src --out-dir lib --source-maps inline",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "clean": "[ -d ./lib ] && rm -r ./lib || echo 'already clean!'",
    "test": "mocha --compilers js:babel-register test/*.js"
  },
  "author": "kamataryo",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "fontmin": "^0.9.6",
    "mocha": "^3.2.0",
    "stream-array": "^1.1.2",
    "stream-assert": "^2.0.3",
    "through2": "^2.0.3",
    "vinyl": "^2.0.1"
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "mocha": "^3.2.0"
  },
  "license": "ISC"
}
